{
    "name": "nyholm/git-reviewer",
    "description": "Find a good reviewer to your github pull request",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nyholm",
            "email": "tobias.nyholm@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/console": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/config": "^5.0",
        "symfony/yaml": "^5.0",
        "symfony/event-dispatcher": "^5.0",
        "symfony/filesystem": "^5.0",
        "symfony/http-client": "^5.1",
        "nyholm/psr7": "^1.3",
        "knplabs/github-api": "^2.15",
        "symfony/process": "^5.1",
        "symfony/cache": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Nyholm\\GitReviewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Nyholm\\GitReviewer\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.1"
    }
}
